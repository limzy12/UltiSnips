snippet beg "begin{} / end{}" bA
\begin{$1}
	$0
\end{$1}
endsnippet

snippet ilist "bulleted list" bA
\begin{itemize}
	\item $0
\end{itemize}
endsnippet

snippet nlist "enumerated list" bA
\begin{enumerate}
	\item $0
\end{enumerate}
endsnippet

snippet i "item in list" b
\item $0
endsnippet

snippet sect "section" bA
\section{$1}
$0
endsnippet

snippet ssect "subsection" bA
\subsection{$1}
$0
endsnippet

snippet sssect "subsubsection" bA
\subsubsection{$1}
$0
endsnippet

snippet tbf "bold text" A
\textbf{$1}$0
endsnippet


snippet tit "italic text" A
\textit{$1}$0
endsnippet
